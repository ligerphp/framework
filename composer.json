{
    "name": "liger/framework",
    "description": "The liger Framework.",
    "keywords": ["framework", "liger"],
    "license": "MIT",
    "homepage": "https://liger.io",
    "support": {
        "issues": "https://github.com/ligerphp/framework/issues",
        "source": "https://github.com/ligerphp/framework"
    },
    "authors": [
        {
            "name": "Alfred Johnson Awah",
            "email": "ligerframework@gmail.com"
        }
    ],
    "require": {
       "liger/framework":"1.0"
    },
    "autoload": {
        "files": [
            "src/Core/Foundation/helpers.php"
        ],
        
        "psr-4": {
            "Core\\": "src/Core/",
            "App\\":"app",
            "Database\\":"database"
        }
    }, "autoload-dev": {
        "psr-4": {
            "Core\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "symfony/dotenv": "^4.2",
        "phpunit/phpunit": "^8",
        "symfony/var-dumper": "^4.2"
    },
    "minimum-stability": "dev"
}
